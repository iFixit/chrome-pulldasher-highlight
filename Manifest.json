{
   "name": "Highlight Pulldasher Tabs",
   "description": "",
   "version": "0.1",
   "background": {
      "scripts": ["background.js"]
   },
   "permissions": [
      "*://pulldasher.cominor.com/*",
      "tabs"
   ],
   "content_scripts": [{
      "matches": ["*://pulldasher.cominor.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
   }],
   "manifest_version": 2
}