{
   "name": "Highlight Pulldasher Tabs",
   "description": "",
   "version": "1.0",
   "background": {
      "scripts": ["config.js", "background.js"]
   },
   "permissions": [
      "*://pulldasher.cominor.com/*",
      "tabs"
   ],
   "content_scripts": [{
      "matches": ["*://pulldasher.cominor.com/*"],
      "js": ["config.js", "content.js"],
      "run_at": "document_end"
   }],
   "manifest_version": 2
}